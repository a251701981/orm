{
    "name": "bravo3/orm",
    "type": "library",
    "description": "NoSQL ORM for databases such as Redis",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordon Scott",
            "email": "jordon@novatek.com.au"
        }
    ],
    "autoload": {
        "psr-4": {
            "": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": ["tests/"]
        }
    },
    "require": {
        "php": ">=5.6.0,<8",
        "psr/log": "^1.0.0",
        "eloquent/enumeration": "^5.1.0",
        "symfony/event-dispatcher": "^3.0.0",
        "doctrine/annotations": "^1.2.0",
        "doctrine/inflector": "^1.0.0",
        "ocramius/proxy-manager": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1.0",
        "predis/predis": "^1.0.0",
        "incenteev/composer-parameter-handler": "^2.0.0",
        "bravo3/properties": "^1.0.2"
    },
    "suggest": {
        "predis/predis": "For Redis support"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "extra": {
        "incenteev-parameters": {
            "file": "tests/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    }
}
